  <target name="compile.module.serialkillerplugin" depends="compile.module.serialkillerplugin.production,compile.module.serialkillerplugin.tests" description="Compile module SerialkillerPlugin"/>
  
  <target name="compile.module.serialkillerplugin.production" depends="register.custom.compilers" description="Compile module SerialkillerPlugin; production classes">
    <mkdir dir="${serialkillerplugin.output.dir}"/>
    <javac2 destdir="${serialkillerplugin.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.serialkillerplugin}/javac">
      <compilerarg line="${compiler.args.serialkillerplugin}"/>
      <bootclasspath refid="serialkillerplugin.module.bootclasspath"/>
      <classpath refid="serialkillerplugin.module.production.classpath"/>
      <src refid="serialkillerplugin.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.serialkillerplugin"/>
    </javac2>
    
    <copy todir="${serialkillerplugin.output.dir}">
      <fileset dir="${module.serialkillerplugin.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.serialkillerplugin.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.serialkillerplugin.tests" depends="register.custom.compilers,compile.module.serialkillerplugin.production" description="compile module SerialkillerPlugin; test classes" unless="skip.tests">
    <mkdir dir="${serialkillerplugin.testoutput.dir}"/>
    <javac2 destdir="${serialkillerplugin.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.serialkillerplugin}/javac">
