      <patternset refid="excluded.from.compilation.serialkillerplugin"/>
    </javac2>
    
    <copy todir="${serialkillerplugin.testoutput.dir}">
      <fileset dir="${module.serialkillerplugin.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.serialkillerplugin.basedir}/src/test/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
