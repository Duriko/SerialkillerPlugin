  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <fileset dir="${javac2.home}">
      <include name="javac2.jar"/>
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="forms-*.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  <import file="${basedir}/module_serialkillerplugin.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.serialkillerplugin" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.serialkillerplugin" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.serialkillerplugin" value="${basedir}/../../Desktop/minecraft/plugins"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.SerialkillerPlugin.jar" value="${artifacts.temp.dir}/SerialkillerPlugin.jar"/>
  </target>
  
  <target name="artifact.serialkillerplugin" depends="init.artifacts" description="Build &#39;SerialkillerPlugin&#39; artifact">
    <property name="artifact.temp.output.serialkillerplugin" value="${artifacts.temp.dir}/SerialkillerPlugin"/>
    <mkdir dir="${artifact.temp.output.serialkillerplugin}"/>
    <jar destfile="${temp.jar.path.SerialkillerPlugin.jar}" duplicate="preserve" filesetmanifest="mergewithoutmain"/>
    <copy file="${temp.jar.path.SerialkillerPlugin.jar}" tofile="${artifact.temp.output.serialkillerplugin}/SerialkillerPlugin.jar"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.serialkillerplugin" description="Build all artifacts">
    <mkdir dir="${artifact.output.serialkillerplugin}"/>
    <copy todir="${artifact.output.serialkillerplugin}">
      <fileset dir="${artifact.temp.output.serialkillerplugin}"/>
    </copy>
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="all" depends="build.modules, build.all.artifacts" description="build all"/>
